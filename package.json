{
  "name": "filechunk-pro",
  "version": "0.1.0",
  "description": "高性能、跨平台的大文件上传解决方案，支持React、Vue、原生JS和小程序环境",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/umd/filechunk-pro.min.js",
  "jsdelivr": "dist/umd/filechunk-pro.min.js",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "build": "pnpm run clean && pnpm run build:types && rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:perf": "jest tests/unit/performance-benchmark.test.ts",
    "docs": "typedoc --out docs/api src",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "file-upload",
    "chunk-upload",
    "large-file",
    "cross-platform",
    "browser",
    "miniapp",
    "react",
    "vue"
  ],
  "author": "codefish",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/XcodeFish/FileChunk-Pro.git"
  },
  "bugs": {
    "url": "https://github.com/XcodeFish/FileChunk-Pro/issues"
  },
  "homepage": "https://github.com/XcodeFish/FileChunk-Pro#readme",
  "packageManager": "pnpm@10.8.0",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/pako": "^2.0.3",
    "@types/react-native": "^0.73.0",
    "@types/spark-md5": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-html": "^8.1.3",
    "eslint-plugin-prettier": "^5.4.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "30.0.0-beta.3",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.41.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.3.4",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/react": "^19.1.6",
    "pako": "^2.1.0",
    "react": "^19.1.0",
    "rxjs": "^7.8.2",
    "spark-md5": "^3.0.2",
    "vue": "^3.5.16"
  },
  "peerDependencies": {
    "react-native": ">=0.60.0",
    "rxjs": "^7.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
